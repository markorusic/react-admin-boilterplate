{
  "name": "react-admin-boilerplate",
  "version": "0.0.0",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc -b",
    "format": "prettier --write .",
    "gen:feature": "ts-node --project scripts/tsconfig.json scripts/generate-feature.ts",
    "gen:trans-types": "ts-node --project scripts/tsconfig.json scripts/generate-translation-types.ts"
  },
  "dependencies": {
    "antd": "^4.18.3",
    "axios": "^1.3.4",
    "dayjs": "^1.11.7",
    "formik": "^2.2.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.8",
    "react-router-dom": "^6.2.1",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@types/inquirer": "^8.1.3",
    "@types/node": "^18.15.11",
    "@types/prettier": "^2.7.2",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "inquirer": "^8.2.0",
    "msw": "^0.36.5",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3",
    "vite": "^4.2.1"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
